{% extends '::base.html.twig' %}

{% block body -%}
    {% include 'DevsparkVentureJuiceBundle:Default:nav.html.twig' with {'nav': 'emails'} %}
    <h1>EmailMessage</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Subject</th>
                <td>{{ entity.subject }}</td>
            </tr>
            <tr>
                <th>Body</th>
                <td>{{ entity.body | raw }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('emailmessage') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('emailmessage_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
